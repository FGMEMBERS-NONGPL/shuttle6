						material.showDialog("sim/model/livery/material/covers/", nil, 450);
						material.showDialog("sim/model/livery/material/ripple/", nil, 670);
				</script>
			</binding>
		</key>

		<!-- rewrite autopilot lock because dg-heading doesn't work with this model -->
		<key n="8">
			<name>Ctrl-H</name>
			<desc>Toggle autopilot heading lock</desc>
			<binding>
				<command>nasal</command>
				<script>
					var node = props.globals.getNode("/autopilot/locks/heading", 1);
					if ( node.getValue() == "true-heading-hold" ) {
						node.setValue( "" );
					} else {
						node.setValue( "true-heading-hold" );
					}
				</script>
			</binding>
		</key>
